<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andorid.hospital">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".IniciarSesionUI"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".PrincipalUI"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" />
        <activity
            android:name=".menuUI"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PacienteUI" />
        <activity android:name=".MedicacionMedicoUI" />
        <activity android:name=".AddMUI" />
        <activity android:name=".AddPacienteUI" />
        <activity android:name=".MensajesUI" />
        <activity android:name=".EstudianteUI" />
        <activity android:name=".AddEstudianteUI" />
        <activity android:name=".ValoracionUI" />
        <activity android:name=".MedicacionEnfermeroUI" />
        <activity android:name=".ListaEstudiantes"></activity>
    </application>

</manifest>